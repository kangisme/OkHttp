<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="HardcodedText"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:id="@+id/custom_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="CustomView"/>

        <Button
            android:id="@+id/custom_view_group"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="CustomViewGroup"/>

        <Button
            android:id="@+id/channel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ChannelCategoryActivity"/>

        <Button
            android:id="@+id/view_draw"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="View的绘制"/>

        <Button
            android:id="@+id/circle_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="圆形进度条"/>

        <Button
            android:id="@+id/recycle_property"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Recycle我的资产"/>

        <Button
            android:id="@+id/floating_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="可拖拽悬浮按钮"/>

        <Button
            android:id="@+id/html5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="HTML5"/>
        <Button
            android:id="@+id/texiaotu"
            android:text="特效图"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>
</ScrollView>
